{
  "name": "zacks-hompeage",
  "version": "0.2.0",
  "description": "A simple homepage for a Zack",
  "private": true,
  "dependencies": {
    "ajv": "^6.5.4",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "popper": "^1.0.1",
    "popper.js": "^1.14.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "1.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zackzachariah/my-homepage"
  },
  "keywords": [
    "node",
    "homepage",
    "react"
  ],
  "scripts": {
    "build-css": "node-sass-chokidar public/sass -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar public/sass -o public/css --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all -p build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "license": "MIT"
}
