{
  "name": "zacks-hompeage",
  "version": "0.2.0",
  "description": "A simple homepage for a Zack",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "^2.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zackzachariah/my-homepage"
  },
  "keywords": [
    "node",
    "homepage",
    "react"
  ],
  "scripts": {
    "build-css": "node-sass-chokidar public/sass -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar public/sass -o public/css --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all -p build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
